mendeley-tags = {BSCO2201,Cuprate,Doping},
month = oct,
pages = {10},
title = {{Bulk and Surface Nanoscale Hole Density Inhomogeneity in HgBa\$\_2\$CuO\$\_\{4+\backslash delta\}\$ and Bi\$\_2\$Sr\$\_2\$CaCu\$\_2\$O\$\_\{8+\backslash delta\}\$ Cuprates}},
