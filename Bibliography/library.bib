journal = {arxiv.org},
keywords = {ARPES,BSCO2201,Cuprate,Fermi Surface,Strongly Correlated Electrons,Superconductivity},
mendeley-tags = {ARPES,BSCO2201,Cuprate,Fermi Surface},
month = dec,
pages = {5},
title = {{Structural origin of apparent Fermi surface pockets in angle-resolved photoemission of Bi\$\_2\$Sr\$\_\{2-x\}\$La\$\_x\$CuO\$\_\{6+\backslash delta\}\$}},
url = {http://arxiv.org/abs/1012.1484},
year = {2010}
}
