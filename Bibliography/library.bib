@article{Yamashita2011,
abstract = {We investigated the structure of the superconducting order parameter in the iron-pnictide superconductor BaFe\_2(As\_\{0.67\}P\_\{0.33\})\_2 (T\_c=31 K) with line nodes by the angle-resolved thermal conductivity measurements in magnetic field rotated within the basal plane of the crystal. We find that the thermal conductivity displays distinct fourfold oscillations with minima when the field is directed at +-45 deg with respect to the tetragonal a axis, namely the Fe-Fe bond direction. We discuss possible gap structures that can account for the data in the context of recent theoretical work, and conclude that the observed results are most consistent with the closed nodal loops located at the flat parts of the electron Fermi surface with high Fermi velocity.},
